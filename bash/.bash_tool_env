#Setup Quartus II
export ALTERA_VERSION=12.0
export ALTERA_ROOTDIR=/project/altera/$ALTERA_VERSION
export QUARTUS_ROOTDIR=$ALTERA_ROOTDIR/quartus/bin
export QII_BASE_DIR=$QUARTUS_ROOTDIR
export MODELSIM_ROOTDIR=$ALTERA_ROOTDIR/modelsim_ae/bin
export QUARTUS_64BIT=1 #Force Q2 to run in 64 bit mode
