[user]
	name = kmurray\n
	email = k.murray@utoronto.ca

[credential]
	helper = cache --timeout=3600

[core]
    excludesfile = ~/.gitignore_global
[color]
	ui = true

[diff]
	tool = tkdiff

[difftool]
	prompt = false

[alias]
    dt = difftool
    st = status
    br = branch
    co = checkout
[push]
	default = matching
